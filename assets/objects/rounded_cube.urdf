<?xml version="1.0"?>
<robot name="urdf_robot">
  <link name="base_link">
    <contact>
      <rolling_friction value="0.0" />
      <spinning_friction value="0.0" />
      <lateral_friction value="10" />
      <restitution value="0.1" />
      <damping value="100" />
      <stiffness value="10000" />
    </contact>
    <inertial>
      <origin rpy="0 0 0" xyz="0 0 0" />
      <mass value="0.05" />
      <inertia ixx="0.1" ixy="0" ixz="0" iyy="0.1" iyz="0" izz="0.1" />
    </inertial>
    <visual>
      <origin rpy="0 0 0" xyz="0 0 0" />
      <geometry>
        <mesh filename="rounded_cube.obj" scale="1 1 1" />
        <!-- <mesh filename="textured_sphere_smooth.obj" scale="0.03 0.03 1"/>       -->
      </geometry>
      <material name="white">
        <color rgba="1 1 1 1" />
      </material>
    </visual>
    <collision>
      <origin rpy="0 0 0" xyz="0 0 0" />
      <geometry>
        <mesh filename="rounded_cube.obj" scale="1 1 1" />
      </geometry>
    </collision>
  </link>
</robot>